<Window x:Class="linguistic_center.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:linguistic_center"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid Margin="0,0,-6.333,-19.333">
        <TabControl HorizontalAlignment="Left" Height="330" VerticalAlignment="Top" Width="525">
            <TabItem Header="Подбор">
                <Grid Background="#FFE5E5E5">
                    <Image Margin="0,0,0.333,0" Source="Image2.png" Stretch="Fill"/>
                    <Frame Content="Подбор языковой группы" HorizontalAlignment="Left" Height="37" Margin="173,10,0,0" VerticalAlignment="Top" Width="173"/>
                    <Frame Content="Выберите язык курса (английский, немецкий, &#xD;&#xA;испанский, французский, китайский, японский):" HorizontalAlignment="Left" Height="42" Margin="10,34,0,0" VerticalAlignment="Top" Width="283"/>
                    <Frame Content="Введите свой уровень владения языком&#xA;(zero , A, B, C):" HorizontalAlignment="Left" Height="37" Margin="10,104,0,0" VerticalAlignment="Top" Width="233"/>
                    <Frame Content="Введите свой возраст:" HorizontalAlignment="Left" Height="34" Margin="10,166,0,0" VerticalAlignment="Top" Width="140"/>
                    <Frame Content="Удобная станция метро&#xD;&#xA;(Тверская, Марксистская, Трубная):" HorizontalAlignment="Left" Height="36" Margin="10,205,0,0" VerticalAlignment="Top" Width="204"/>
                    <TextBox HorizontalAlignment="Left" Height="19" Margin="94,122,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="66"/>
                    <TextBox HorizontalAlignment="Left" Height="20" Margin="140,166,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="50"/>
                    <TextBox HorizontalAlignment="Left" Height="25" Margin="10,246,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="188"/>
                    <TextBox HorizontalAlignment="Left" Height="23" Margin="10,76,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="188"/>
                    <Button Content="Подобрать" HorizontalAlignment="Left" Height="29" Margin="392,251,0,0" VerticalAlignment="Top" Width="99"/>
                    <Frame Content="Результат поиска:" HorizontalAlignment="Left" Height="30" Margin="298,69,0,0" VerticalAlignment="Top" Width="167"/>
                    <TextBox HorizontalAlignment="Left" Height="133" Margin="298,99,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="182"/>
                </Grid>
            </TabItem>
            <TabItem Header="TabItem">
                <Grid Background="#FFE5E5E5" Margin="0,0,0.333,0">
                    <Image Margin="-70,0,-569,-224" Source="Image3.png" Stretch="Fill"/>
                    <TextBox HorizontalAlignment="Left" Height="29" Margin="80,77,0,0" TextWrapping="Wrap" Text="Введите пароль:" VerticalAlignment="Top" Width="149"/>
                    <PasswordBox HorizontalAlignment="Left" Height="29" Margin="277,77,0,0" VerticalAlignment="Top" Width="147"/>
                    <Label Content="                 Для сотрудников лингивистического центра" HorizontalAlignment="Left" Height="30" Margin="80,10,0,0" VerticalAlignment="Top" Width="366"/>
                </Grid>
            </TabItem>
        </TabControl>
    </Grid>
</Window>
